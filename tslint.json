{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": false,
    "ordered-imports": [
      false
    ]
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  }
}
